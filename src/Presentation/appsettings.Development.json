{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgresDb": "dotnet user-secrets set ConnectionStrings:PostgresDb <connection_string>"
  },
  "Authentication": {
    "Discord": {
      "ClientId": "dotnet user-secrets set Authentication:Discord:ClientId <client_id>",
      "ClientSecret": "dotnet user-secrets set Authentication:Discord:ClientSecret <client_secret>"
    }
  },
  "Frontend": {
    "Url": "http://localhost:4200"
  }
}